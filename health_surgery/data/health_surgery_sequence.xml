<odoo>
<data noupdate="1">
<!-- Sequences for Surgery Codes -->
    <record id="seq_type_medical_surgery_code" model="ir.sequence.type">
        <field name="name">Surgery</field>
        <field name="code">medical.surgery</field>
    </record>
    <record id="seq_medical_surgery_code" model="ir.sequence">
        <field name="name">Surgery</field>
        <field name="code">medical.surgery</field>
        <field name="prefix">SRG-${year}-</field>
        <field name="padding">5</field>
    </record>
</data>
